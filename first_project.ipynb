     ]
    }
   ],
   "source": [
    "# Adding a column with the order year and month\n",
    "\n",
    "failed_orders['year_month'] = pd.to_datetime(failed_orders['order_purchase_timestamp']).dt.to_period('M')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Grouping by month and status, counting the number of orders\n",
    "\n",
    "failed_orders = failed_orders.groupby(['year_month', 'order_status'], as_index=False) \\\n",
    "                             .agg({'order_id': 'count'}) \\\n",
    "                             .rename(columns={'order_id': 'orders'})"
   ]
  },
